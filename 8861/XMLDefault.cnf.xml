<?xml version="1.0" encoding="UTF-8"?>
<!-- 
XMLDefault.cnf.xml
Default Configuration File for Cisco SIP IP Phones
This file is used for initial phone setup and firmware specification
-->
<Default>
  <callManagerGroup>
    <members>
      <member priority="0">
        <callManager>
          <ports>
            <ethernetPhonePort>2000</ethernetPhonePort>
            <sipPort>5060</sipPort>
            <securedSipPort>5061</securedSipPort>
          </ports>
          <processNodeName><my ip></processNodeName>
        </callManager>
      </member>
    </members>
  </callManagerGroup>

  <!-- Firmware Load Information for Different Phone Models -->
  <!-- Cisco 7900 Series -->
  <loadInformation8 model="Cisco IP Phone 7940">P0S3-08-12-00</loadInformation8>
  <loadInformation7 model="Cisco IP Phone 7960">P0S3-08-12-00</loadInformation7>
  
  <!-- Cisco 7906/7911 -->
  <loadInformation30018 model="Cisco IP Phone 7906G">SCCP11.9-4-2SR3-1S</loadInformation30018>
  <loadInformation307 model="Cisco IP Phone 7911">SCCP11.9-4-2SR3-1S</loadInformation307>
  
  <!-- Cisco 7941/7942 -->
  <loadInformation115 model="Cisco IP Phone 7941">SIP41.9-4-2SR3-1S</loadInformation115>
  <loadInformation309 model="Cisco IP Phone 7942">SIP42.9-4-2SR3-1S</loadInformation309>
  
  <!-- Cisco 7945 -->
  <loadInformation437 model="Cisco IP Phone 7945">SIP45.9-4-2SR3-1S</loadInformation437>
  
  <!-- Cisco 7961/7962 -->
  <loadInformation30016 model="Cisco IP Phone 7961">SIP41.9-4-2SR3-1S</loadInformation30016>
  <loadInformation404 model="Cisco IP Phone 7962">SIP42.9-4-2SR3-1S</loadInformation404>
  
  <!-- Cisco 7965 -->
  <loadInformation436 model="Cisco IP Phone 7965">SIP45.9-4-2SR3-1S</loadInformation436>
  
  <!-- Cisco 7970 -->
  <loadInformation30006 model="Cisco IP Phone 7970">SIP70.9-4-2SR3-1S</loadInformation30006>
  
  <!-- Cisco 7971 -->
  <loadInformation119 model="Cisco IP Phone 7971">SIP70.9-4-2SR3-1S</loadInformation119>
  
  <!-- Cisco 7975 -->
  <loadInformation435 model="Cisco IP Phone 7975">SIP75.9-4-2SR3-1S</loadInformation435>
  
  <!-- Cisco 8800 Series (newer models) -->
  <loadInformation562 model="Cisco IP Phone 8841">sip88xx.12-5-1SR1-4</loadInformation562>
  <loadInformation563 model="Cisco IP Phone 8845">sip88xx.12-5-1SR1-4</loadInformation563>
  <loadInformation564 model="Cisco IP Phone 8861">sip88xx.12-5-1SR1-4</loadInformation564>

  <!-- URL Configuration -->
  <authenticationURL></authenticationURL>
  <directoryURL></directoryURL>
  <idleURL></idleURL>
  <informationURL></informationURL>
  <messagesURL></messagesURL>
  <proxyServerURL></proxyServerURL>
  <servicesURL></servicesURL>

  <!-- VLAN Settings -->
  <vlanConfiguration>false</vlanConfiguration>

  <!-- DSCP Settings -->
  <dscpForSCCPPhoneConfig>96</dscpForSCCPPhoneConfig>
  <dscpForSCCPPhoneServices>0</dscpForSCCPPhoneServices>
  <dscpForCm2Dvce>96</dscpForCm2Dvce>

  <!-- Transport Protocol (4 = TCP+UDP) -->
  <transportLayerProtocol>4</transportLayerProtocol>

  <!-- Certificate Authority Proxy Function -->
  <capfAuthMode>0</capfAuthMode>
  <capfList>
    <capf>
      <phonePort>3804</phonePort>
    </capf>
  </capfList>
  <certHash></certHash>
  <encrConfig>false</encrConfig>

  <!-- Time Zone -->
  <timeZone>Asia/Riyadh</timeZone>

  <!-- NTP Server -->
  <ntpServer1>pool.ntp.org</ntpServer1>
  <ntpServer2>time.nist.gov</ntpServer2>

  <!-- Dial Plan -->
  <dialTemplate>dialplan.xml</dialTemplate>

  <!-- Softkeys -->
  <softKeyFile>softkeys.xml</softKeyFile>

</Default>
