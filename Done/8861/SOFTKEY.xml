  <softKeySets>
    <!-- Phone is idle, on hook -->
    <softKeySet id="On Hook">
      <softKey keyID="Redial" />
      <softKey keyID="NewCall" />
      <softKey keyID="PickUp" />
      <softKey keyID="GPickUp" />
      <softKey keyID="CFwdAll" />
      <softKey keyID="DND" />
    </softKeySet>
    
    <!-- Phone is off hook, ready to dial -->
    <softKeySet id="Off Hook">
      <softKey keyID="Redial" />
      <softKey keyID="EndCall" />
      <softKey keyID="PickUp" />
      <softKey keyID="CFwdAll" />
      <softKey keyID="MeetMe" />
    </softKeySet>
    
    <!-- Off hook with a feature active -->
    <softKeySet id="Off Hook With Feature">
      <softKey keyID="Redial" />
      <softKey keyID="EndCall" />
      <softKey keyID="&lt;&lt;" />
    </softKeySet>
    
    <!-- User has entered digits -->
    <softKeySet id="Digits After First">
      <softKey keyID="Dial" />
      <softKey keyID="&lt;&lt;" />
      <softKey keyID="EndCall" />
    </softKeySet>
    
    <!-- Outbound call is ringing -->
    <softKeySet id="Ring Out">
      <softKey keyID="EndCall" />
      <softKey keyID="Transfer" />
      <softKey keyID="CallBack" />
    </softKeySet>
    
    <!-- Call is connected/active -->
    <softKeySet id="Connected">
      <softKey keyID="Hold" />
      <softKey keyID="EndCall" />
      <softKey keyID="Transfer" />
      <softKey keyID="Confrn" />
      <softKey keyID="Park" />
      <softKey keyID="ConfList" />
      <softKey keyID="Select" />
    </softKeySet>
    
    <!-- Connected but no features available -->
    <softKeySet id="Connected No Feature">
      <softKey keyID="Hold" />
      <softKey keyID="EndCall" />
    </softKeySet>
    
    <!-- Connected and in transfer mode -->
    <softKeySet id="Connected Transfer">
      <softKey keyID="Transfer" />
      <softKey keyID="EndCall" />
      <softKey keyID="Dial" />
      <softKey keyID="&lt;&lt;" />
    </softKeySet>
    
    <!-- Connected and in conference mode -->
    <softKeySet id="Connected Conference">
      <softKey keyID="Confrn" />
      <softKey keyID="EndCall" />
      <softKey keyID="ConfList" />
      <softKey keyID="Join" />
      <softKey keyID="RmLstC" />
    </softKeySet>
    
    <!-- Call is on hold -->
    <softKeySet id="On Hold">
      <softKey keyID="Resume" />
      <softKey keyID="NewCall" />
      <softKey keyID="Transfer" />
      <softKey keyID="Confrn" />
      <softKey keyID="Swap" />
      <softKey keyID="Join" />
      <softKey keyID="Select" />
    </softKeySet>
    
    <!-- Incoming call is ringing -->
    <softKeySet id="Ring In">
      <softKey keyID="Answer" />
      <softKey keyID="iDivert" />
      <softKey keyID="DND" />
    </softKeySet>
    
    <!-- Remote in use (shared line) -->
    <softKeySet id="Remote In Use">
      <softKey keyID="NewCall" />
      <softKey keyID="Barge" />
      <softKey keyID="cBarge" />
      <softKey keyID="Redial" />
    </softKeySet>
    
  </softKeySets>
</softKeyCfg>
